<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Resources/GeneralStyles.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <Style x:Key="UPtextBox" BasedOn="{StaticResource StyledTextBox}" TargetType="TextBox">
        
        <Setter Property="Width" Value="320"/>
    </Style>
    <Style x:Key="UPlabel" BasedOn="{StaticResource StyledLabel}" TargetType="Label">

        <Setter Property="Width" Value="320"/>
    </Style>
</ResourceDictionary>